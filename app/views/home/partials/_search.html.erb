 <h1>Search#filter</h1>
<p>Find me in app/views/search/filter.html.erb</p>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.name %></td>
        <td><%= product.description %></td>
      </tr>
    <% end %>
  </tbody>
</table> 